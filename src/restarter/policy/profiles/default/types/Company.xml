<?xml version="1.0"?>
<object name="Company"
   meta_type="Factory-based Type Information with dynamic views"
   i18n:domain="restarter.policy"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Company</property>
 <property name="description" i18n:translate="">restartER company</property>
 <property name="icon_expr">string:${portal_url}/folder_icon.gif</property>
 <property name="content_meta_type">Company</property>
 <property name="product">restarter.policy</property>
 <property name="factory">addCompany</property>
 <property name="add_view_expr"></property>
 <property name="link_target"></property>
 <property name="immediate_view">atct_edit</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="Collection"/>
  <element value="Document"/>
  <element value="Folder"/>
  <element value="Image"/>
  <element value="Link"/>
  <element value="Products"/>
  <element value="Topic"/>
 </property>
 <property name="allow_discussion">True</property>
 <property name="default_view">base_view</property>
 <property name="view_methods">
  <element value="base_view"/>
 </property>
 <property name="default_view_fallback">False</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="atct_edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    icon_expr="" link_target="" url_expr="string:${folder_url}/"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    icon_expr="" link_target="" url_expr="string:${object_url}/edit"
    visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
