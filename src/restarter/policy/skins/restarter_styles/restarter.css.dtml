/* <dtml-with base_properties> (do not remove this :) */
/* <dtml-call "REQUEST.set('portal_url', portal_url())"> (not this either :) */

p {
    line-height: 1.5em;
}
h3, h4, h5, h6 {
    font-family: <dtml-var fontFamily>;
}
h1, h2 {
    letter-spacing: -0.05em;
}
textarea,
#content textarea {
    font-family: <dtml-var fontFamily>;
}
img.image-left, img.image-right, .captioned img {
    border:0;
}
.state-soldout {
    color: &dtml-statusSoldout; !important;
}
#plone-contentmenu-workflow .state-soldout {
    padding: 0 0.5em;
    background-color: &dtml-statusSoldout;;
    border: 0.1em solid &dtml-statusSoldout;;
    color: white !important;
    border-radius: 0.5em;
    -webkit-border-radius: 0.5em;
    -moz-border-radius: 0.5em;
}
#plone-contentmenu-workflow .state-draft {
    padding: 0 0.5em;
    background-color: &dtml-statusDraft;;
    border: 0.1em solid &dtml-statusDraft;;
    color: white !important;
    border-radius: 0.5em;
    -webkit-border-radius: 0.5em;
    -moz-border-radius: 0.5em;
}

.state-published {
    color: &dtml-statusPublished; !important;
}
.state-accepted {
    color: &dtml-statusAccepted; !important;
}
.state-draft {
    color: &dtml-statusDraft; !important;
}
#plone-contentmenu-workflow .state-accepted {
    padding: 0 0.5em;
    background-color: &dtml-statusAccepted;;
    border: 0.1em solid &dtml-statusAccepted;;
    color: white !important;
    border-radius: 0.5em;
    -webkit-border-radius: 0.5em;
    -moz-border-radius: 0.5em;
}

#portal-header {
    background-color: #444C4C;
    color: #848C8B;
}
#inner-portal-header,
#logo-nav-wrapper,
#nav-wrapper ul,
#slider,
#portal-columns,
#inner-portal-footer,
#colophonWrapper {
    max-width: 980px;
    margin: 0 auto;
    position: relative;
}
#inner-portal-header a {
    color: #848C8B;
}
#inner-portal-header a:hover {
    color: #FFF;
}

/** search **/
#portal-searchbox {
    float: left;
    background-color: #576262;
    margin:0;
    padding: 14px 10px 0;
    min-height: 42px;
}
.LSBox .searchSection {
    display: none;
}
#portal-searchbox #searchGadget,
#portal-searchbox #nolivesearchGadget,
#faceted-form div.faceted-text-widget input[type="text"] {
    background: #F0F0F0 none;
    border:0;
     -moz-border-radius: 6px 0 0 6px;
    -webkit-border-radius: 6px 0 0 6px;
    border-radius: 6px 0 0 6px; 
    padding: 6px 10px;
    width: 9em;
    color: #444C4C;
}
 #portal-searchbox .LSBox input.searchButton,
 #faceted-form div.faceted-text-widget input[type="submit"] {
    background: #F0F0F0 url(++resource++restarter.images/ico_lente.png) no-repeat center center;
    border:0;
     -moz-border-radius: 0 6px 6px 0;
    -webkit-border-radius: 0 6px 6px 0;
    border-radius: 0 6px 6px 0; 
    padding: 6px 20px;
    margin-left: -4px;
    text-indent: -9999em;
}
#faceted-form div.faceted-text-widget input[type="text"],
#faceted-form div.faceted-text-widget input[type="submit"] {
    float: left;
}
#faceted-form div.faceted-text-widget div {
    clear: left;
}

/** personal bar **/
#portal-personaltools,
#anon-portal-personaltools {
    background-color: #576262;
    float: left;
    padding: 20px 10px 0 0;
    -moz-border-radius: 0;
    -webkit-border-radius: 0;
    border-radius: 0; 
    min-height: 36px;
    font-size: 0.9em;
}
#personaltools-login,
#personaltools-join {
    font-style: italic;
}
#personaltools-join {
    display: none;
}
#anon-portal-personaltools #anon-personalbar a {
    margin:0;
}
#portal-personaltools li a,
#anon-portal-personaltools li a {
    border-left: 1px dotted #848C8B;
    padding: 0.2em 1em;
    margin: 0 0 0 0;
}
#anon-portal-personaltools #personaltools-login {
    border:0;
}
dl.actionMenu#portal-personaltools {
    margin-left: 0;
    padding-left: 10px;
    background-color: &dtml-contentViewBackgroundColor;;
}
#portal-personaltools dt.actionMenuHeader a, #portal-personaltools dt.actionMenuHeader a,
#portal-personaltools dt.actionMenuHeader a:hover, #portal-personaltools dt.actionMenuHeader a:hover {
    color: #FFF;
}
#portal-personaltools dd,
#anon-portal-personaltools dd {
    background-color: &dtml-contentViewBackgroundColor;;
    left: 10px;
    right: auto;
    top: 42px;
    border-top: 3px solid #CCC;
}
#portal-personaltools dd li a,
#anon-portal-personaltools dd li a {
    border:0;
    color: #FFF;
}
span.noMenuAction {
    color: #FFF;
}

/** globalnav **/
#portal-globalnav {
    background-color: transparent;
    margin:0;
    position: absolute;
    right:0;
    top:18px;
    font-size: 1.25em;
    font-family: <dtml-var headingFontFamily>;
}
#portal-globalnav li {
    display: inline-block;
    height: 39px;
}
#portal-globalnav li a {
    background-color: transparent;
    padding: 0 0.7em;
    border-right: 1px dotted #848C8B;
    min-width: 1px;
}
#portal-globalnav li.selected {
    border-bottom: 3px solid #FFF;
}
#portal-globalnav li#portaltab-index_html.selected {
    border-bottom-color: #ED1C24;
}
#portal-globalnav .selected a,
#portal-globalnav a:focus,
#portal-globalnav a:hover {
    background-color: transparent;
}
#portal-globalnav .selected a:focus,
#portal-globalnav .selected a:hover {
    background-color: transparent;
    color: #FFF;
}
#portal-globalnav li#portaltab-contact a {
    border-right: 0;
    background-image: url(++resource++restarter.images/ico_mail.png);
    background-repeat: no-repeat;
    background-position: right center;
    padding-right: 30px;
}

/** logo **/
#portal-logo {
    margin: 25px 0 0;
    float: left;
}
#portal-column-one #portal-logo {
    margin:0 0 20px;
    float: none;
    position: relative;
    top:0;
    left:0;
}


/** big nav red **/
#navRedBtn.bigBtn {
    float: right;
}
#logo-nav-wrapper {
    padding: 40px 0;
}
.template-facetednavigation_view #logo-nav-wrapper {
    padding-bottom: 0;
}
#navRedBtn.bigBtn ul#navRed li {
    display: block;
    float: left;
    width: 181px;
    height: 181px;
    background: #EC1B24 url(++resource++restarter.images/bg_gradient_red.png) repeat-x left top;
    border-bottom: 3px solid #444C4C;
    margin: 0 5px;
}
#navRedBtn.bigBtn ul#navRed li a {
    display: block;
    height: 161px;
    background: transparent url(++resource++restarter.images/sign_down.png) no-repeat center 90%;
    color: #FFF;
    font-family: <dtml-var headingFontFamily>;
    font-size: 2.6em;
    padding: 10px;
}
#navRedBtn.bigBtn ul#navRed li a:hover {
    background-color: #900;
}
#nav-wrapper {
    background-color: &dtml-colorRed;;
    text-align: right;
    padding: 8px 0;
}
#nav-wrapper li {
    display: inline-block;
    margin: 0 0.3em 0 0.1em;
    padding: 0.2em 0 0.2em;
}
#nav-wrapper li a {
    color: #FFF;
    font-family: <dtml-var headingFontFamily>;
    display: inline-block;
    font-size: 2.3em;
    line-height: 1.1em;
    border-left: 1px dotted #FFF;
    padding: 0 0.6em;
}
#nav-wrapper li#navBtnProdotti a {
    border-left:0;
}
#nav-wrapper li a:hover {
    color: #FF9;
}
/** slider **/
#slider-wrapper {
    background-color: #F0F0F0;
}
#slider {
    border-bottom: 3px solid &dtml-colorRed;;
}
#sliderContent {
    background: transparent url(&dtml-portal_url;/slider_img_collage.jpg) no-repeat left top;
    padding: 0 0 0 77%;
    min-height: 300px;
}
#sliderTxt {
    background-color: #F0F0F0;
    padding: 10px;
}
#sliderTxt h2 {
    font-size: 2em;
    line-height: 1.2em;
    margin-bottom: 0.4em;
}
#sliderTxt p {
    font-style: italic;
}
p.btnLink {
    margin-top: 2em;
}
p.btnLink a.btn,
p.noOffers a.btn {
    display: inline-block;
    background-color: &dtml-linkColor;;
     -moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    border-radius: 6px;
    color: #FFF;
    font-style: normal; 
    font-family: <dtml-var headingFontFamily>;
    font-size: 1.7em;
    text-transform: uppercase;
    padding: 0.4em 1em;
}
p.btnLink a.btn:hover,
p.noOffers a.btn:hover {
    background-color: #CDCDCD;
    cursor: pointer;
}

/** footer **/
#portal-footer-wrapper {
    clear: both;
    background-color: #444C4C;   
    color: #FFF;
}
#portal-footer-wrapper a {
    color: #FFF;
}
#portal-footer-wrapper a:hover,
#portal-siteactions li a:focus, #portal-siteactions li a:hover {
    color: #FF9;
}
#portal-footer {
    background-color: #576262;
    color: #2A2F2F;
    font-family: <dtml-var headingFontFamily>;
}
#portal-colophon {
    font-size: 0.9em;
    text-align: left;
    margin-bottom: 0;
    padding-bottom: 0.5em;
}
.logoFooter {
    display: block;
    margin: 5px 0;
}
#logo-rer {
    margin-bottom: 10px;
}
p.seguici,
p.sostegno {
    font-size: 1.4em;  
    line-height: 1.2em; 
}
p.seguici {
    margin-bottom: 0.4em;
}
li.socialLink {
    display: block;
    font-size: 1.2em;
    padding-left: 35px;
    background-position: left 100px;
    background-repeat: no-repeat;
    background-color: transparent;
    background-image: url(++resource++restarter.images/ico_socials_small.png);
    margin: 5px 0;
}
li#linkFB {
    background-position: left 3px;
}
li#linkTW {
    background-position: left -57px;
}
.footerColumn p.btnLink {
    margin-top: 1em;
}
.footerColumn p.btnLink a.btn {
    font-size: 1.4em;
    background-color: #444C4C;
}
.footerColumn p.btnLink a.btn:hover {
   background-color: #999;
   color: #444c4c !important; 
}
.footerColumn {
    width: 32%;
    margin: 0;
    float: left;
}
#footer-column-2,
#colophon-column-2 {
    margin: 0 2%;
}
#colophon-column-1,
#colophon-column-2 {
    padding-top: 1em;
}
a.faq {
    display: block;
    background: transparent url(++resource++restarter.images/ico_question.png) no-repeat left center;
    padding: 0.3em 0 0.3em 40px;
    line-height: 1.4em;
}
#portal-siteactions {
    margin-left: -4em;
}
#portal-siteactions li {
    border-left: 1px dotted #EEE;
    margin-right: 0;
}
#portal-siteactions li#siteaction-privacy {
    border-left:0;
}

/** breadcrumbs **/
#breadcrumbs-you-are-here {
    display: none;
}
.section-homepage #portal-breadcrumbs {
    display: none;
}
#portal-breadcrumbs {
    font-size: 0.9em;
    margin-left:0;
    color: #000;
    margin-bottom: 0.4em;
}
#portal-breadcrumbs a,
#portal-breadcrumbs #breadcrumbs-current {
    font-style: italic;
}
.breadcrumbSeparator {
    padding: 0 0.2em;
}
/** contenuto **/
#portal-columns {
    clear: both;
    padding: 30px 0 20px;
}
body#content.mceContentBody {
    font-size: 84%;
}
.link-parent {
    margin: 0.5em 0;
    padding: 0.1em 0.5em;
    background-color: #F0F0F0;
    display: block;
}
#viewlet-social-like iframe {
   width: 100px !important;
}
#content .documentDescription, #content #description {
    font-family: <dtml-var fontFamily>;
    margin-bottom: 1em;
    line-height: 1.5em;
}
#plone-document-byline {
    padding: 0.5em 0 2em;
    border-top: 3px solid #EEE;
}
#content h1.documentFirstHeading {
    line-height: 1.3em;
    padding-bottom: 0.3em;
    margin-bottom: 0.5em;
    background: transparent url(++resource++restarter.images/border_title.png) no-repeat left bottom;
}
.pullquote,
#content .pullquote {
    color: black;
    font-size: 1.4em;
    line-height: 1.3em;
    letter-spacing: -0.06em !important;
    font-family: <dtml-var headingFontFamily>;
}
.callout,
#content .callout {
    background-color: #666;
    border:0;
    color: #FFF;
    font-size: 1.1em;
}
#content .field textarea[name$=".description"], #archetypes-fieldname-description textarea#description {
    color: #000;
    font-size: 1em;
    font-weight: normal;
}

/** social **/
#viewlet-social-like {
    border-top: 3px solid #F0F0F0;
    padding-top: 10px
}
.portaltype-product #viewlet-social-like,
.portaltype-order #viewlet-social-like {
    border:0;
}

/** Portlets **/
dl.portlet dt {
    font-family: <dtml-var headingFontFamily>;
    font-size: 1.8em;
    padding: 0 0 0.3em;
    margin: 0 0 0.5em 0.6em;
    letter-spacing: -0.05em;
    background: transparent url(++resource++restarter.images/border_title.png) no-repeat left bottom;
}
dl.portlet dt .tile,
dl.portlet dt a.tile,
#content dl.portlet dt.portletHeader a.tile {
    color: #000;
}
dl.portlet dd.portletItem {
    padding: 0.5em;
}
dl.rtCollectionPortlet dd.portletItem {
    background-color: #fafafa;
    border-left: 1px dotted #CCC;
}
dl.portletCalendar dt {
    font-size: 1.3em;
    background-image: none;
}

/*** twitter **/
.twtr-widget {
    border: 1px solid #F0F0F0;
    padding: 10px;
}
.portlet .twtr-hd,
.portletStaticText .twtr-hd {
    background: transparent url(++resource++restarter.images/ico_socials_big.png) no-repeat left -150px;
    padding-left: 60px;
    border-bottom: 1px solid #F0F0F0;
}
.twtr-ft div {
    background-color: #F0F0F0;    
    font-size: 0.95em;
    font-style: italic;
}
.twtr-ft div a {
    color: #999 !important;    
}
/** facebook **/
.fb_iframe_widget {
    border: 1px solid #F0F0F0;
}

/** navigazione **/
dl.portletERPortletNavigation dd.portletItem {
    background-color: #FAFAFA;
    padding:0.5em 0.3em 0;
}
dl.portletERPortletNavigation {
    border-bottom: 3px solid #F0F0F0;
    padding-bottom: 0;
    font-size: 1.05em;
}
dl.portletERPortletNavigation ul.navTreeLevel0 {
    margin-left: 0.5em;
}
dl.portletERPortletNavigation ul.navTree li.navTreeItem {
    padding: 0 0.7em;
    background-image: url(++resource++restarter.images/nav-dot.png);
    background-repeat. no-repeat;
    background-position: left 0.7em;
    background-repeat: no-repeat;
    padding-left: 12px;
}
dl.portletERPortletNavigation ul.navTree li.navTreeTopNode {
    font-weight: bold;
}
dl.portletERPortletNavigation ul.navTree li.navTreeItem a {
    padding: 0.4em 0;
    color: #000;
    line-height: 1.3em;
}
dl.portletERPortletNavigation ul.navTreeLevel1 li.navTreeItem {
    background-image: url(++resource++restarter.images/indent.gif);
    background-repeat. no-repeat;
    background-position: left 0.2em;
    background-repeat: no-repeat;
    padding-left: 18px;
}
dl.portlet ul.navTree .navTreeCurrentItem {
    margin-left: 0;
    margin-right: 0;
    background-color: transparent;
    border-left: 0;
    font-style: italic;
}
dl.portletERPortletNavigation ul.navTree li.navTreeItem a.navTreeCurrentItem {
    color: &dtml-linkColor;;
}
dl.portlet p {
    margin-bottom: 0.4em;
}
#content dl.portlet, dl.portlet,
div.rerPortletAdvancedStatic,
div.portletStaticText,
dl.portletStaticText {
    margin-bottom: 20px;
}

/** portlet utlime notizie **/
.faCollectionPortlet .portletHeader a.linkMore {
    font-size: 0.5em;
    letter-spacing: normal;
    font-family: <dtml-var fontFamily>;
    color: &dtml-linkColor;;
    border-left: 1px dotted &dtml-linkColor;;
    padding: 0  0 0 0.5em;
    font-weight: normal;
    font-style: italic;
    float: right;
    margin-top: 0.8em;
}
.faCollectionImg {
    float: left;
    margin: 0 0.8em 0.1em 0;
    width: 100px;
    height: 100px;
    border-bottom: 3px solid &dtml-linkColor;;
    overflow: hidden;
}
h3.faCollectionTitle {
    font-weight: normal;
    font-size: 1.2em;
    line-height: 1.3em;
    margin-bottom: 0.3em;
}
.faCollectionPortlet .itemData {
    margin: 0 0 0.3em;
    font-size: 0.9em;
    color: #000;
    font-style: italic;
}
p.faCollectionDesc {
    font-size: 0.95em;
    font-style: italic;
    margin-bottom:0;
}
.newsPortlet .portletItem {
    background-color: #FAFAFA;
    padding: 0.8em;
    margin: 10px 0;
    border-left: 1px dotted #CCC;
}

/** ultimi prodotti **/
dl.productsPortlet dd.portletItem,
#portletPageColumns dl.productsPortlet dd.portletItem,
#content .facetedResult {
    float: left;
    width: 30%;
    margin: 0 1.5% 20px;
    padding:0;
    max-width: 200px;
    min-height: 320px;
}
#portletPageColumns dl.productsPortlet dd.portletItem {
    min-height: 1px;
}
#content .offerResult {
    min-height: 150px;
}
#content dl.portlet dt.portletHeader {
    margin: 0 0 0.5em 0.6em;
}
.productsPortlet .faCollectionImg,
.productResult .faCollectionImg {
    float: none;
    margin: 0 auto 0.5em;
    width: 100%;
}
#content dl.productsPortlet p.itemData,
#content .productResult p.itemData {
    margin: 0 0 0.3em;
}
#content dl.productsPortlet p.faCollectionDesc, 
dl.productsPortlet p.faCollectionDesc,
#content .facetedResult p.faCollectionDesc {
    margin-bottom: 0.5em;
}
#content dl.productsPortlet p.productPrice, 
dl.productsPortlet p.productPrice,
#content .productResult p.productPrice {
    font-size: 0.9em;
    color: #000;
    font-style: italic;
    margin-bottom: 1em;
}
dl.productsPortlet p.productPrice .euro,
#content .productResult p.productPrice .euro {
    font-family: <dtml-var headingFontFamily>;
    font-size: 1.5em;
    font-style: normal;
    font-weight: bold;
}
dl.productsPortlet p.btnLink,
#content .facetedResult p.btnLink {
    margin:0;
}
dl.productsPortlet p.btnLink a.btn,
#content dl.productsPortlet p.btnLink a.btn,
#content .facetedResult p.btnLink a.btn {
    background-color: #CCC;
    color: #FFF;
    font-size: 1.2em;
}
#content .productResult p.btnLink a.btn {
    font-size: 1em;
    padding: 0.35em 0.6em;
}
dl.productsPortlet p.btnLink a.btn:hover,
#content dl.productsPortlet p.btnLink a.btn:hover,
#content .facetedResult  p.btnLink a.btn:hover {
    background-color: &dtml-linkColor;;
    color: #FFF !important;
}
#content dl.productsPortlet {
    border-bottom: 3px solid #F0F0F0;
    margin-bottom: 0.7em;
}
#content dl.productsPortlet dd.portletItem {
    margin-bottom: 10px;
}
#content dl.offersPortlet dd.portletItem {
    min-height: 150px;
}
#content #portletPageColumns dl.offersPortlet dd.portletItem {
    min-height: 1px;   
}
#content dl.offersPortlet p.faCollectionDesc {
    margin-bottom: 0.8em;
}
#content dl.productsPortlet dt.portletHeader {
    margin-bottom: 0.8em;
}

/** company **/
.companyResult,
#content .companyResult {
    text-align: center;
    float: left;
    width: 20%;
    margin: 0 1.5% 20px;
    max-width: 150px;
    min-height: 165px;
    padding: 5px;
    border: 1px solid #F0F0F0;
}
.companyResult .faCompanyImg {
    float: none;
    margin: 0 0 0.5em;
    width: auto;
    height: auto;
    border-bottom: 0;
}
.companyResult .faCompanyImg a {
    display: block;
    text-align: center;
    width: 128px;
    height: 110px;
    overflow: hidden;
    margin: 0 auto;
}
.companyResult .faCompanyImg a img {
    vertical-align: middle;
}
#content .companyResult .faCompanyImg a.noFoto {
    height: 110px;
}
#content .companyResult .faCompanyImg a.noFoto span {
    top: 33%;
}
/** Company **/

.portaltype-company #content .tileImage {
    float: left;
    margin-left: 0;
    padding: 0.3em 0.5em 0.2em 0;
}
.portaltype-company #content .documentDescription {
    font-weight: normal;
    color: &dtml-fontColor;
}
#companyBody {
    float: left; 
    padding-top: 0;
    max-width: 305px;
}
div.comMap a {
    margin: 0.3em 0 0;
    font-size: 0.9em;
    padding-left: 15px;
    background: transparent url(++resource++restarter.images/map_pin.png) no-repeat left center;
}
div.comMap img {
    border: 1px solid #F0F0F0;
    padding: 10px;
}
div.comMap {
    padding: 20px 0 10px;
    border-top: 3px solid #F0F0F0;
    text-align: center;
    margin: 0 1em;
}
#content .companyRec {
    margin: 0 0 0;
    float: left;
    color: #000;
}
.companyRec .labelTxt {
    font-weight: bold;
}
.companyRec h2 {
    padding-bottom: 0.2em;
}
.comAddressWrapper div {
    display: inline;
}
.comAddressWrapper {
    font-size: 1.1em;
}
.comAddressWrapper .compAddressStreet {
    display: block;
