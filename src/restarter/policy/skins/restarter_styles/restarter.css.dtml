/* green */
.green,
body.template-register #actionsView .actionButtons input.context,
#order-edit-view .formControls input.context[type="submit"],
#order-edit-view .formControls input.standalone[type="submit"],
#login-form .registerBlock .btnRegister a.green {
    color: #e8f0de;
    border: solid 1px #538312;
    background: #64991e;
    background: -webkit-gradient(linear, left top, left bottom, from(#7db72f), to(#4e7d0e));
    background: -moz-linear-gradient(top,  #7db72f,  #4e7d0e);
    filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#7db72f', endColorstr='#4e7d0e');
}
.green:hover,
body.template-register #actionsView .actionButtons input.context:hover,
#order-edit-view .formControls input.context[type="submit"]:hover,
#order-edit-view .formControls input.standalone[type="submit"]:hover,
#login-form .registerBlock .btnRegister a.green:hover {
    background: #538018;
    background: -webkit-gradient(linear, left top, left bottom, from(#6b9d28), to(#436b0c));
    background: -moz-linear-gradient(top,  #6b9d28,  #436b0c);
    filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#6b9d28', endColorstr='#436b0c');
}
.green:active,
body.template-register #actionsView .actionButtons input.context:active,
#order-edit-view .formControls input.context[type="submit"]:active,
#order-edit-view .formControls input.standalone[type="submit"]:active,
#login-form .registerBlock .btnRegister a.green:active {
    color: #a9c08c;
    background: -webkit-gradient(linear, left top, left bottom, from(#4e7d0e), to(#7db72f));
    background: -moz-linear-gradient(top,  #4e7d0e,  #7db72f);
    filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#4e7d0e', endColorstr='#7db72f');
}
/* white */
.white,
#order-edit-view .formControls input.standalone[type="submit"],
#rpx-login-container fieldset#login-form div.formControls input.context {
    color: #606060;
    border: solid 1px #b7b7b7;
