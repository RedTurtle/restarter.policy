    padding: 10px;
    background-color: #F0F0F0;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    max-width: 120px;
    position: relative;
}
#list-associations .icoCheck {
    width:64px;
    height: 64px;
    overflow: hidden;
    position: absolute;
    top: -24px;
    left: -12px;
    background: transparent url(++resource++restarter.images/icon_check.png) no-repeat left top;
}
